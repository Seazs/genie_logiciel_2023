<?xml version="1.0" encoding="UTF-8"?>

<?import java.net.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="600.0" prefWidth="500.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ulb.infof307.g12.view.paquets.CarteEtudeVueController">
   <children>
      <Button fx:id="boutonPrecedent" layoutX="58.0" layoutY="427.0" mnemonicParsing="false" onAction="#cartePrecedente" text="Précédente">
